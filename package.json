    "url": "https://github.com/RajeshKumarMahto/ReactBasedCalculatorAutomationTest/issues"
  },
  "homepage": "https://github.com/RajeshKumarMahto/ReactBasedCalculatorAutomationTest#readme",
  "devDependencies": {
    "cypress": "^8.2.0",
    "mocha": "^9.0.3",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0"
  }
}
